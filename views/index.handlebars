<div class="container">
	<div class="row">
		<div class="col-md-12 text-center" id="top-bar">
			<img src="data:image/jpeg;base64," alt="burger" />
			<h1> Burger Solutions </h1>
		</div>
		<div class="col-md-6 text-right" class="task">
			<div class="row">
			  {{#each burger_data}}
			  	{{#unless this.devoured}}
				<div class="col-md-9 text-right">
					<pre>{{this.id}}. {{this.burger_name}}
					</pre>
				</div>
				<div class="col-md-3 text-center">
					<form class="devour-form button-size">
						<input input type="hidden" class="burger_id" type="text" value={{this.id}}><br>
						<button type="submit" class="btn btn-default">Devour the Burger!</button>
					</form>
				</div>
				{{/unless}}
			  {{/each}}
			</div>
		</div>
	</div>
</div>

<div class="container">
	<div class="row">
		<div class="col-md-12 text-center enter_section">
			<form action="/burger" method="POST">
				<input type="text" class="form-control" name="burger_name" value="" id="enter_text">
				<button type="submit" class="btn btn-primary" id="text-enter-button">Submit</button>
			</form>
			<p>Enter the burger you want and click "Submit".</p>
		</div>
		<div class="col-md-6 text-center" class="task">
			  {{#each burger_data}}
			  	{{#if this.devoured}}
			  		<input class="form-control" type="text" placeholder="{{this.id}} . {{this.burger_name}}" readonly>
				{{/if}}
			  {{/each}}
		</div>
	</div>
</div>
